{
	"format_version": "1.8.0",
	"minecraft:entity": {
		"description": {
			"identifier": "mob:safe",
			"runtime_identifier":"minecraft:ender_crystal",
			"is_spawnable": true,
			"is_summonable": true,
			"is_experimental": false,
			"scripts":{
			   "animate":["chest"]
			},
			"animations":{
			  "chest": "controller.animation.chest"
			}
			
		},
		"component_groups": {
			"untamed": {
				"minecraft:interact": {
					"on_interact": {
						"filters": {
							"all_of": [
								{"test": "has_equipment","subject": "other"	,"operator": "!=","value": "minecraft:gold_ingot"}
							]
						},
						"event": "kill"
					},
					"interact_text": "撤去"
				},
				"minecraft:tameable": {
					"probability": 1.0,
					"tameItems": "minecraft:gold_ingot",
					"tame_event": {
						"event": "tamed",
						"target": "self"
					}
				}
			},
			"lock_controller": {
				"minecraft:is_tamed": {
				},
				"minecraft:interact": [
					{
						"on_interact": {
							"filters": {
								"all_of": [
									{"test": "is_owner"			,"subject": "other"	,"value": true},
									{"test": "is_mark_variant"	,"subject": "self"	,"value": 101},
									{"test": "has_equipment"	,"subject": "other"	,"operator": "!=","value": "minecraft:iron_ingot"}
								]
							},
							"event": "unlocking"
						},
						"interact_text": "action.interact.safe.unlock"
					},
					{
						"on_interact": {
							"filters": {
								"all_of": [
									{"test": "has_tag"			,"subject": "other"	,"value": "leader"},
									{"test": "is_mark_variant"	,"subject": "self"	,"value": 101},
									{"test": "has_equipment"	,"subject": "other"	,"operator": "!=","value": "minecraft:iron_ingot"}
								]
							},
							"event": "unlocking"
						},
						"interact_text": "action.interact.safe.unlock"
					},
					{
						"on_interact": {
							"filters": {
								"all_of": [
									{"test": "is_sneaking"		,"subject": "other"	,"value": true},
									{"test": "is_owner"			,"subject": "other"	,"value": true},
									{"test": "has_equipment"	,"subject": "other"	,"value": "minecraft:iron_ingot"}
								]
							},
							"event": "kill"
						},
						"interact_text": "撤去"
					}
				]
			},
			"locking": {
				"minecraft:timer": {
				  "looping": true,
				  "time": 0.4,
				  "time_down_event": {
					"event": "locked"
				  }
				},
				"minecraft:is_saddled": {
				},
				"minecraft:mark_variant": {
					"value": 100
				},
				"minecraft:inventory": {
					"container_type": "minecart_chest",
					"inventory_size": 1000,
					"private": true
				}
			},
			"locked": {
				"minecraft:is_saddled": {
				},
				"minecraft:mark_variant": {
					"value": 101
				},
				"minecraft:inventory": {
					"container_type": "minecart_chest",
					"inventory_size": 1000,
					"private": true
				}
			},
			"unlocking": {
				"minecraft:timer": {
				  "looping": true,
				  "time": 0.4,
				  "time_down_event": {
					"event": "unlocked"
				  }
				},
				"minecraft:mark_variant": {
					"value": 102
				},
				"minecraft:inventory": {
					"container_type": "minecart_chest",
					"inventory_size": 1000,
					"private": false
				}
			},
			"unlocked": {
				"minecraft:timer": {
				  "looping": true,
				  "time": 2.0,
				  "time_down_event": {
					"event": "locking"
				  }
				},
				"minecraft:mark_variant": {
					"value": 103
				},
				"minecraft:inventory": {
					"container_type": "minecart_chest",
					"inventory_size": 1000,
					"private": false
				}
			},
			"kill": {
				"minecraft:despawn": {
				}
			}
		},
		"components": {
			"minecraft:collision_box": {
				"width": 0.4,
				"height": 0.5
			},
     	
			"minecraft:damage_sensor": {
				"triggers": [
				{
					"on_damage": {
					},
					"damage_multiplier": 0.0,
					"deals_damage": false
				}
				]
			},
			"minecraft:loot": {
			  "table": "loot_tables/entities/safe.json"
			},
     		"minecraft:scale": {
      			"value": 0.6
    		},
			"minecraft:health": {
				"value": 10,
				"max": 10
			},
			"minecraft:nameable":{},
			"minecraft:push_through": {
				"value": 1
			},
			"minecraft:pushable": {
			  "is_pushable": false,
			  "is_pushable_by_piston": false
			},
			"minecraft:knockback_resistance": {
				"value": 1.0,
				"max": 1.0
			}
		},
		"events": {
			"minecraft:entity_spawned": {
				"add": {
					"component_groups": [ "untamed", "locked" ]
				}
			},
			"tamed": {
				"add": {
					"component_groups": [ "lock_controller" ]
				},
				"remove": {
					"component_groups": [ "untamed" ]
				}
			},
			"locking": {
				"add": {"component_groups": [ "locking" ]},
				"remove": {"component_groups": [ "locked" ]}
			},
			"locked": {
				"add": {"component_groups": [ "locked" ]},
				"remove": {"component_groups": [ "locking" ]}
			},
			"unlocking": {
				"add": {"component_groups": [ "unlocking" ]},
				"remove": {"component_groups": [ "locked" ]}
			},
			"unlocked": {
				"add": {"component_groups": [ "unlocked" ]},
				"remove": {"component_groups": [ "unlocking" ]}
			},
		
			"kill": {
				"add": {
					"component_groups": [ "kill" ]
				}
			}
		}
	}
}